//V1
class Obstacle : Tile
    {
        //Create a constructor that recieves and sets the intial values
        public Obstacle(int x, int y, char symbol) : base(x, y, symbol)
        {
            this.X = x;
            this.Y = y;
            this.Symbol = symbol;
        }
    }
