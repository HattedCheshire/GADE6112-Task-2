//V1
//Create the EmptyTile subclass (derived) from Tile
    class EmptyTile : Tile
    {
        //Create a constructor that recieves and sets the intial values
        public EmptyTile(int x, int y, char symbol) : base(x, y, symbol)
        {
            this.X = x;
            this.Y = y;
            this.Symbol = symbol;
        }
    }
//V2
